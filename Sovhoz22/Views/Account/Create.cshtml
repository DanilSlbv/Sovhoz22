@model Sovhoz22.Models.UserModels.CreateUserViewModel
@{ 
    ViewData["Title"] = "Создание";
}

<h2>Создание пользователя</h2>

<form asp-action="Create" asp-controller="Account">
    <div asp-validation-summary="ModelOnly"></div>
    <div class="form-group">
        <label asp-for="Email">Email</label>
        <input type="email" asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Year">Год рождения</label>
        <input type="number" asp-for="Year" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Password">Пароль</label>
        <input type="password" asp-for="Password" class="form-control" />
    </div>
    <div>
        <input type="submit" value="Создать" class=" btn btn-default"/>
    </div>
</form>